{
    "format_version": 1,
    "tooth": "github.com/tooth-hub/iland",
    "version": "2.8.4",
    "dependencies": {
            "github.com/tooth-hub/liteloaderbds": [
				["2.9.x"]
			]
	},
    "information": {
        "name": "iLand",
        "description": "Powerful land plugin, based on LiteLoader.",
        "author": "RedbeanW",
        "license": "GPLv3",
        "homepage": "https://github.com/LiteLScript-Dev/iLand-Core"
    },
    "placement": [
        {
            "source": "iland-core.lua",
            "destination": "plugins/iland-core.lua"
        },
		{
            "source": "iland/*",
            "destination": "plugins/iland/*"
        },
		{
            "source": "lib/*",
            "destination": "plugins/lib/*"
        },
    ],
	"possession": [
		"plugins/iland/"
	]
}